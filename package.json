  "bugs": {
    "url": "https://github.com/leonleo997/protractor-workshop-2018/issues"
  },
  "homepage": "https://github.com/leonleo997/protractor-workshop-2018#readme",
  "dependencies": {
    "jasmine-spec-reporter": "^4.2.1",
    "protractor": "^5.4.4",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1"
  },
  "devDependencies": {
    "@types/jasminewd2": "^2.0.6",
